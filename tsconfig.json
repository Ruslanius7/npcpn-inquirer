{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/utils/*": ["src/utils/*"],
      "~/components/*": ["src/components/*"],
      "~/layouts/*": ["src/layouts/*"],
      "~/api/*": ["src/api/*"],
      "~/schemas/*": ["src/schemas/*"],
      "~/constants": ["src/constants"]
    }
  }
}
